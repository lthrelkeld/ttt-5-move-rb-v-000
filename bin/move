#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," ",]

puts "Where would you like to go?"
input = gets.strip

new_index = input_to_index(input)
new_board = move(board,new_index)
display_board(new_board)
